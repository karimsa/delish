{
  "name": "delish",
  "version": "0.0.1",
  "description": "Find your love. Then eat it.",
  "main": "index.js",
  "engines": {
    "node": "7.x"
  },
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 index.js",
    "postinstall": "./node_modules/.bin/bower install && npm run setup",
    "setup": "if [ -z \"$NODE_ENV\" ] || [ \"$NODE_ENV\" == \"production\" ]; then ./node_modules/precommit/bin/cli.js install; fi",
    "watch": "concurrently 'node index.js' 'gulp watch'",
    "pretest": "gulp lint:css lint:js",
    "test": "echo \"There aren't actually any unit tests ... but let's pass.\""
  },
  "precommit": [
    "npm run pretest"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karimsa/delish.git"
  },
  "author": "Karim Alibhai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karimsa/delish/issues"
  },
  "homepage": "https://github.com/karimsa/delish#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "concurrently": "^3.4.0",
    "cssnano": "^3.10.0",
    "debounce": "^1.0.0",
    "eslint-config-strict": "^13.0.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^3.0.1",
    "gulp-imagemin": "^3.1.1",
    "gulp-jslint": "^1.0.10",
    "gulp-postcss": "^6.3.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-stylelint": "^3.9.0",
    "gulp-uglify": "^2.1.0",
    "precommit": "^1.2.2",
    "precss": "^1.4.0",
    "stylelint": "^7.9.0",
    "stylelint-config-suitcss": "^10.0.0",
    "stylelint-order": "^0.4.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "bower": "^1.8.0",
    "browserify": "^14.1.0",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "socket.io": "^1.7.3"
  }
}
